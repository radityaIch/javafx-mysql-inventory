<AnchorPane id="AnchorPane" fx:id="ApBarang" maxHeight="480.0" maxWidth="640.0" minHeight="480.0" minWidth="640.0" prefHeight="266.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gui.programinventaris.InventarisManajemenController">
   <children>
      <ScrollPane layoutX="248.0" layoutY="83.0" prefHeight="337.0" prefWidth="365.0">
