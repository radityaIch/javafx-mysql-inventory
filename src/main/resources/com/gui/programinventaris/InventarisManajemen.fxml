          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="333.0" prefWidth="362.0">
               <children>
                  <TableView fx:id="tbview" prefHeight="330.0" prefWidth="365.0">
                    <columns>
                        <TableColumn fx:id="tcIdBarang" prefWidth="65.0" text="ID Barang" />
                      <TableColumn fx:id="tcNama" prefWidth="209.0" text="Nama Barang" />
                      <TableColumn fx:id="tcJumlah" prefWidth="89.0" text="Jumlah" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <TextField id="jumlahBarang" fx:id="jumlahBarang" layoutX="27.0" layoutY="142.0" prefHeight="26.0" prefWidth="193.0" promptText="Jumlah" />
      <Button id="tambahBarang" fx:id="btnTambah" layoutX="27.0" layoutY="197.0" mnemonicParsing="false" onAction="#nampilin" prefHeight="30.0" prefWidth="193.0" text="Tambah Barang" />
